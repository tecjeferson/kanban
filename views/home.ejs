<%- include('partials/header') -%>



<div class="container mt-5">

    <div class="row mt-3">
        <div class="col-lg-3">
            <h3>Today</h3>


            <% dados.forEach(function(dados) { %>

            <% if(dados.Priority == 1){ %>

            <div class="shadow-sm p-3 mb-2 table-primary rounded">
                <td>

                    <%= dados.TaskName %>
                </td>
                <div class="row">
                    <div class="col">
                        <i id="icons" class="material-icons">
                            more edit delete_forever
                        </i>
                    </div>
                </div>

            </div>
            <% }if(dados.Priority == 2){ %>
            <div class="shadow-sm p-3 mb-2 table-warning rounded">
                <td>
                    <%= dados.TaskName %>
                    <div class="row">
                        <div class="col">
                            <i id="icons" class="material-icons">
                                more edit delete_forever
                            </i>
                        </div>
                    </div>
                </td>
            </div>
            <% }if(dados.Priority == 3){ %>
            <div class="shadow-sm p-3 mb-2 table-danger rounded">
                <td>
                    <%= dados.TaskName %>
                    <div class="row">
                        <div class="col">
                            <i id="icons" class="material-icons">
                                more edit delete_forever
                            </i>
                        </div>
                    </div>

                </td>
            </div>
            <% } %>

            <% }); %>


        </div>

        <div class="col-lg-3">
            <h3>Tomorrow</h3>


            <% dados.forEach(function(dados) { %>

            <% if(dados.Priority == 1){ %>

            <div class="shadow-sm p-3 mb-2 table-primary rounded">
                <td>

                    <%= dados.TaskName %>
                </td>
                <div class="row">
                    <div class="col">
                        <i id="icons" class="material-icons">
                            more edit delete_forever
                        </i>
                    </div>
                </div>

            </div>
            <% }if(dados.Priority == 2){ %>
            <div class="shadow-sm p-3 mb-2 table-warning rounded">
                <td>
                    <%= dados.TaskName %>
                    <div class="row">
                        <div class="col">
                            <i id="icons" class="material-icons">
                                more edit delete_forever
                            </i>
                        </div>
                    </div>
                </td>
            </div>
            <% }if(dados.Priority == 3){ %>
            <div class="shadow-sm p-3 mb-2 table-danger rounded">
                <td>
                    <%= dados.TaskName %>
                    <div class="row">
                        <div class="col">
                            <i id="icons" class="material-icons">
                                more edit delete_forever
                            </i>
                        </div>
                    </div>

                </td>
            </div>
            <% } %>

            <% }); %>


        </div>
    </div>





</div>
<%- include('partials/footer'); -%>